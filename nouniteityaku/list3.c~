#include <stdio.h>
#include <stdlib.h>

int max(int x, int y);

int main(void)
{
  int a, b;
  int (*pM)(int x, int y);

  a=100;
  b=222222;
  
  pM=max;

  printf("%d\n", max(a,b));
  printf("%d\n", (*pM)(a,b));
  printf("%d\n", pM(a,b));

  
  return 0;
}

int max(int x, int y){
  if (x>y){
    return x;
  } else {
    return y;
  }
