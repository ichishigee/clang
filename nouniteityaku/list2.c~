#include <stdio.h>

void swap(int *x, int *y){
  int tmp;
  tmp=*x;
  *x=*y;
  *y=tmp;
}

void add (int *x, int *y) {
  *x=+2;
  *y=+2;
}


int main (void)
{
  int num1=10;
  int num2=20;
  int tmp;
  
  printf("%d\n", num1);
  printf("%d\n", num2);

  add(&num1, &num2);
 
  printf("%d\n", num1);
  printf("%d\n", num2);

  swap(&num1, &num2);

  
  printf("%d\n", num1);
  printf("%d\n", num2);
  
  return 0;
