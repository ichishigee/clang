
#include<stdio.h>
#define MAX(x, y) (x>y ? x : y)

int main(void){

  int num1,num2, sum;

  scanf("%d", &num1);
  scanf("%d", &num2);

  sum = buy(num1, num2);
  printf("%d\n", sum);

  printf("%d\n", MAX(num1, num2));
  return 0;
}



int buy(int i ,int j){  
  printf("%d %d\n",i,j);
  return i + j;
}


int max(int i ,int j){
  if(i>j)
    return i;
  else
    return j;
